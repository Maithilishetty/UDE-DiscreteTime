function op = ud(u)
    global Ts T G
    t = u(3);
    xm = [u(1);u(2)];
    x = [u(4);u(5)];
    
    if mod(t, Ts) == 0
        G_inv = (1\(G'*G))*G';
        error = x - xm;
        disp(
        disp(-G_inv((Ts/T)*error))
        %op = -G_inv((Ts/T)*error);
        %disp(op)
    else
        op = 0;
    end
    
